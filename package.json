	"devDependencies": {
		"eslint": "^8.41.0",
		"typescript": "^5.0.4"
	},
	"dependencies": {
		"@nuogz/i18n": "^3.2.1",
		"@nuogz/utility": "^1.7.0",
		"axios": "^1.4.0"
