{
	"name": "@nuogz/aegis",
	"version": "1.4.1",
	"description": "HTTP request library for browser and Node.js based on axios",
	"author": "Nuogz",
	"license": "LGPL-3.0-or-later",
	"repository": {
		"type": "git",
		"url": "github:nuogz/aegis"
	},
	"type": "module",
	"typesSource": [
		"browser"
	],
	"engines": {
		"node": ">=14.18.1"
	},
	"sideEffects": false,
	"main": "./src/index.js",
	"browser": {
